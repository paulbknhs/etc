#pmlogconf-setup 2.0
ident   metrics used by the pcp-pidstat(1) command
probe   proc.psinfo.rss values ? include : exclude
delta	60 seconds

	proc.nprocs
	proc.psinfo.pid
	proc.psinfo.cmd
	proc.psinfo.utime
	proc.psinfo.stime
	proc.psinfo.guest_time
	proc.psinfo.processor
	proc.psinfo.rt_priority
	proc.psinfo.policy
	proc.psinfo.minflt
	proc.psinfo.maj_flt
	proc.psinfo.vsize
	proc.psinfo.rss
	proc.psinfo.wchan_s
	proc.memory.vmstack
	proc.id.uid
	proc.id.uid_nm
	proc.psinfo.psargs
