#
# pcp-dstat(1) configuration file - see pcp-dstat(5)
#

[proc]
label = procs
width = 3
printtype = f
colorstep = 10
running = kernel.all.running - 1
running.label = run
blocked = kernel.all.blocked
blocked.label = blk
sysfork = kernel.all.sysfork
sysfork.label = new

[proc-count]
label = procs
width = 4
scalestep = 10
total = proc.nprocs
